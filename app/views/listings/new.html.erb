<h1>Add a New Listing</h1>

<%= form_tag "http://localhost:3000/home" do %>
	
	<%= label_tag "Item:" %>
	<%= text_field_tag "item" %>
	
	<%= label_tag "Price:" %>
	<%= text_field_tag "price" %>
	
	<%= label_tag "Location:" %>
	<%= text_field_tag "location" %>
	
	<%= submit_tag "Create Listing" %>
	
	<% end %>